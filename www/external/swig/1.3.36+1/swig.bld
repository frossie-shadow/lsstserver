# LSST build script for swig 1.3.36+1
#    (patched)
#
tarfile=swig-1.3.36.tar.gz

fetch external/swig/${version}/$tarfile
unpack_tar_and_enter $tarfile

# grab the patch file
echo Pulling down patched source file
fetch external/swig/${version}/python-patched.cxx
mv python-patched.cxx Source/Modules/python.cxx

simplemake
